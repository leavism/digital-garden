{
	"name": "digital-garden",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"auth:generate": "npx @better-auth/cli generate --config src/server/auth/index.ts --output src/server/db/schema/auth.ts && echo '\nYou may have to make changes to the generated schema before pushing to the database.'",
		"build": "next build",
		"check": "biome check .",
