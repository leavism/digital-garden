		"@trpc/server": "^11.1.0",
		"better-auth": "^1.2.7",
		"drizzle-orm": "^0.42.0",
		"framer-motion": "^12.7.4",
		"mysql2": "^3.14.0",
		"next": "^15.2.3",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-hook-form": "^7.56.1",
		"superjson": "^2.2.2",
